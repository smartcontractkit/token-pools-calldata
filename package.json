{
  "name": "@chainlink/token-pools-calldata",
  "version": "1.0.0",
  "description": "Token pools calldata project with ethers v6",
  "engines": {
    "node": ">=22.0.0"
  },
  "bin": {
    "token-pools-calldata": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "prettier --write \"**/*.{ts,json}\"",
    "format:check": "prettier --check \"**/*.{ts,json}\"",
    "typechain": "typechain --target ethers-v6 --out-dir src/typechain \"./abis/**/*.json\"",
    "prepare": "husky",
    "postinstall": "pnpm typechain",
    "start": "ts-node src/cli.ts",
    "check:deps": "pnpm outdated",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@safe-global/types-kit": "^3.0.0",
    "@solana/web3.js": "^1.98.4",
    "commander": "^14.0.2",
    "ethers": "^6.15.0",
    "prettier": "^3.7.3",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
