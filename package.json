{
  "name": "token-pools-calldata",
  "version": "1.0.0",
  "description": "Token pools calldata project with ethers v6",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "token-pools-calldata": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "format:check": "prettier --check \"**/*.{ts,json}\"",
    "typechain": "typechain --target ethers-v6 --out-dir src/typechain \"./abis/**/*.json\"",
    "prepare": "husky",
    "postinstall": "pnpm typechain",
    "start": "ts-node src/cli.ts",
    "check:deps": "pnpm outdated",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^30.0.5",
    "lint-staged": "^15.2.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@safe-global/types-kit": "^1.0.4",
    "@solana/web3.js": "^1.98.2",
    "commander": "^13.1.0",
    "ethers": "^6.11.1",
    "prettier": "^3.5.2",
    "winston": "^3.11.0",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
